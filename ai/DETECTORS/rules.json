{
  "version": 1,
  "detectors": [
    {
      "id": "qtwebengine_present",
      "title": "Qt WebEngine detected",
      "severity": "info",
      "match": {
        "any_text_in_files": [
          "QWebEngineView",
          "Qt6::WebEngineWidgets",
          "Qt5::WebEngineWidgets",
          "qtwebengine"
        ]
      },
      "suggest_recipes": [
        "qtweb-graph-force"
      ]
    },
    {
      "id": "md_not_rendered",
      "title": "Markdown likely opened as plain text (heuristic)",
      "severity": "warn",
      "match": {
        "any_text_in_files": [
          "QTextEdit::setPlainText",
          "plainText",
          ".md",
          "openFile("
        ]
      },
      "suggest_recipes": [
        "md-render-webengine"
      ]
    },
    {
      "id": "no_backstack",
      "title": "No navigation backstack / breadcrumbs (heuristic)",
      "severity": "warn",
      "match": {
        "any_text_in_files": [
          "openNode(",
          "openPath(",
          "load(QUrl",
          "setUrl("
        ]
      },
      "suggest_recipes": [
        "navigation-backstack"
      ]
    }
  ]
}